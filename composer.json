{
    "name": "medz/phpwind",
    "type": "project",
    "description": "采用PHP+MySQL方式运行的开源社区程序",
    "keywords": ["medz", "php", "phpwind"],
    "homepage": "http://www.phpwind.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Seven Du",
            "homepage": "http://medz.cn"
        },
        {

            "name": "phpwind",
            "homepage": "http://www.phpwind.net",
            "role": "Development company"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mcrypt": "*",
        "ext-zlib": "*",
        "ext-zip": "*",
        "medz/windframework": "^1.0",
        "guzzlehttp/guzzle": "^6.2",
        "laravel/framework": "^5.4"
    },
    "autoload": {
        "classmap": [
            "phpwind9/src/Wekit.php",
            "phpwind9/src/library/",
            "phpwind9/src/service/",
            "phpwind9/src/bootstrap/",
            "phpwind9/src/hooks/"
        ],
        "psr-4": {
            "Medz\\Wind\\": "src/",
            "Pw\\Ext\\": "src/extensions/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
